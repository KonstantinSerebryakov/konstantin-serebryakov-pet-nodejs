{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "rules": {
    "@typescript-eslint/no-namespace": "off"
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": { "@typescript-eslint/no-namespace": "off" }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": { "@typescript-eslint/no-namespace": "off" }
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": { "@typescript-eslint/no-namespace": "off" }
    }
  ]
}
